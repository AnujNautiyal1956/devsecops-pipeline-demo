[req]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn

[ dn ]
C=US
ST=Georgia
L=Buford
O=SJ Technologies
OU=Demo
emailAddress={{ gitlab_default_email }}
CN = {{ gitlab_hostname }}

[ req_ext ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
IP.1 = {{ gitlab_ip_address }}